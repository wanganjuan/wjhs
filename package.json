{
	"name": "hellowj",
	"displayName": "helloWJ",
	"publisher": "wjhs",
	"icon": "images/icon.png",
	"description": "",
	"version": "0.4.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.ccc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.ccc",
				"title": "ccc"
			}
		],
		"viewsContainers": {
			"activitybar": [
					{
							"id": "package-explorer",
							"title": "Package Explorer",
							"icon": "images/icon.png"
					}
			]
	},
	"views": {
			"package-explorer": [
					{
							"id": "package-dependencies",
							"name": "Dependencies"
					},
					{
							"id": "package-outline",
							"name": "Outline"
					}
			]
	}
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/wanganjuan/wjhs.git"
    },
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
